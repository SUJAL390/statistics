# Install required packages
install.packages(c("readr", "dplyr", "ggplot2", "caret", "xgboost", "e1071", "corrplot"))
# List of packages required
packages <- c("readr", "dplyr", "ggplot2", "caret", "xgboost", "e1071", "corrplot", "knitr", "kableExtra", "tidyr")
# Install the packages
install.packages(packages)
# List of required packages
required_packages <- c(
"readr", "dplyr", "ggplot2", "caret", "xgboost", "e1071", "corrplot", "knitr", "kableExtra"
)
# Install missing packages
installed_packages <- rownames(installed.packages())
for (pkg in required_packages) {
if (!(pkg %in% installed_packages)) {
install.packages(pkg, dependencies = TRUE)
}
}
# Load all required packages
lapply(required_packages, library, character.only = TRUE)
install.packages('tidyr')
